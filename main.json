{
  "printWidth": 130,
  "tabWidth": 2,
  "singleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "semi": false,
  "useTabs": false,
  "proseWrap": "preserve",
  "arrowParens": "always",
  "htmlWhitespaceSensitivity": "ignore",
  "endOfLine": "auto",
  "quoteProps": "as-needed",
  "vueIndentScriptAndStyle": true,
  "singleAttributePerLine": true
}
